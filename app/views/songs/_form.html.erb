
<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
      <%= f.label :artist %>
  <%= f.text_area :artist %><br><br>
  <%= f.label :genre %>
  <%= f.text_area :genre %><br><br>
    <%= f.label :year %>
  <%= f.text_area :year %><br><br>
  <%= f.submit %>
  <br>
<% end %>
