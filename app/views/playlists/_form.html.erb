
<%= form_for @playlist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label :description %>
  <%= f.text_area :description %><br><br>
  <%= f.label "Original Author" %>
  <%= f.collection_select(:author_id, User.all, :id, :dj_name) %><br><br>
  <%= f.submit %>
  <br>
<% end %>
